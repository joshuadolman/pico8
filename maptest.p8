pico-8 cartridge // http://www.pico-8.com
version 18
__lua__

cam={
	x=0,
	y=0,
	spd=2,
}

function _update60()
	if btn(❎) then
		cam.spd=5
	else
		cam.spd=2
	end
	if btn(⬅️) then
		cam.x-=cam.spd
	end
	if btn(➡️) then
		cam.x+=cam.spd
	end
	if btn(⬆️) then
		cam.y-=cam.spd
	end
	if btn(⬇️) then
		cam.y+=cam.spd
	end
	
	camera(cam.x,cam.y)
end

function _draw()
	cls()
	map(0,0,0,0,128,64)
end
