--update--
function _update60()
 if (gamestate=="startmenu") then
  update_startmenu()
 elseif (gamestate=="game") then
  update_game()
 else
  local p
  if (gamestate=="") then
  p="nil"
  else
  p=gamestate
  end
  debug=("gamestate error-\""..p.."\"")
  _draw()
 end
end

function update_startmenu()
end

function update_game()
	-- ✽boost
 if btn(4) and (boost_active==true) then
  player_accel=0.4
  player_max_dx=3
  boost_amount=mid(0,boost_amount-4,boost_amount_max) else
  player_accel=0.1
  player_max_dx=1
 end
	
 if btn(4)==false then
  boost_amount=mid(0,boost_amount+1,boost_amount_max)
 end

	-- ✽shooting
 if btn(5) and shot_cooldown==0 then
  sfx(0,0)
  shot_cooldown=20
  add(bullet_x,player_x)
  add(bullet_y,player_y)
 end
	
 if (shot_cooldown>0) then
  shot_cooldown-=1
 end

 for k,v in pairs(bullet_y) do
  bullet_y[k]=bullet_y[k]-2
  if (bullet_y[k]<4) then
   destroybullet(k)
  end 
 end
	
	-- ✽movement
 if btn(0) then
  player_dx=mid(0,player_dx+(-player_accel),-player_max_dx)
  flamesprite_offset=5
 elseif  btn(1)  then
  player_dx=mid(0,player_dx+player_accel,player_max_dx)
  flamesprite_offset=7
 elseif btn(0)==false and btn(1)==false then
  player_dx=player_dx/1.1
  flamesprite_offset=3
 end

 player_x=(mid(-1,player_x+player_dx,121))

	--if (stat(19)==-1)and(sqrt(player_dx^2)>0.1) then
	-- sfx(1, 0)
	--elseif (stat(19==1))and(sqrt(player_dx^2)<0.1) then
	-- sfx(-1,0)
	-- sfx(3,0)  
	--end
	
 checkbulletcol()
 frame=frame+1
end

function destroybullet(i)
 del(bullet_x,bullet_x[i])
 del(bullet_y,bullet_y[i])
end

function damagebarrier(i)
 barrier_hp[i]=barrier_hp[i]-1
 barrier_y[i]=barrier_y[i]-2
	
 if barrier_hp[i]==1 then
  del(barrier_x , barrier_x[i])
  del(barrier_y , barrier_y[i])
  del(barrier_hp,barrier_hp[i])
 end
end

function checkbulletcol()
	--bullet sprite offset x & y
 offx=blt_col[1] offy=blt_col[2]
	--bullet col width & height
 wid=blt_col[3] hei=blt_col[4]
 for i,o in pairs(bullet_x) do
  for k,l in pairs(barrier_x) do
   --if not (#bullet_x)==0 and not (#bullet_y)==0 and not (#barrier_x)==0 and not (#barrier_y)==0 then
    local ax=o+offx
    local ay=0
    if not (bullet_y[i]==nil) and not (offy==nil) then
     ay=bullet_y[i]+offy
    end
    local aw=wid local ah=hei
    local bx=l+brr_col[1]
    local by=barrier_y[k]+brr_col[2]
    local bw=brr_col[3]
    local bh=brr_col[4]
    if colchk(ax,ay,aw,ah,bx,by,bw,bh)==true then
		--printh("hallo!")
     destroybullet(i)
     damagebarrier(k)
    end
   --end
  end
 end
end

function colchk(ax,ay,aw,ah,bx,by,bw,bh)
 if ay>by+bh then
  return false
 end
	
 if ay+ah<by then
  return false
 end
	
 if ax>bx+bw then
  return false
 end
	
 if ax+ah<bx then
  return false
 end
 testcol=8
		--printh("bullet destroyed")
 return true
end






