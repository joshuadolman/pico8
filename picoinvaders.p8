--init--
function _init()
 --cls()
 player_x=60
 placebarriers()
 placeenemies()
 music(0)
end

function placebarriers()
 local i
  barrier_x ={}
  barrier_y ={}
  barrier_hp={}
 for i=1,4 do
  add(barrier_x,13+((i-1)*32))
  add(barrier_y,106)
  add(barrier_hp,5)
 end
end

function placeenemies()
 for i=1,9 do
  add(enemies,createenemy("basic",i))
 end
end


