function createbullet(px)
 local bullet={
  x=px,
  y=0
 }
 return bullet
end

function createenemy(t,i)
 local enemy={
  t   =t, --sproffset should be 96
  x   =i*12+3,
  y   =30,
  dx  =0,
  dy  =0,
  hp  =0,
  anim=flr(rnd(4)),
  animoff=flr(rnd(2)),
  animspd=flr(rnd(4))+4,
  col =0
 }
 return enemy
end


