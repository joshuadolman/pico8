function draw_debug()
  local p=("err:"..debug)
  local lines=ceil(#p/31)
  local h=lines*6 
  
  rectfill(0,127-h,127,127,8)
  line(1,128-h,1,126,9)
  
  for i=1,lines do
   print(sub(p,i*31-30,i*31),3,((128-h)+(6*(i-1))),7)
  end
end

function draw_player()
 local fs=flamesprite+flamesprite_offset
 spr(fs,player_x,player_y+3)
 spr(1,player_x,player_y)

 if (frame%8)==0 then
  flamesprite+=1
  if (flamesprite==2) then
	  flamesprite=0
  end
 end
end

function draw_enemies()
 local i
 for i=1,#enemies do
  spr(96+enemies[i].anim,enemies[i].x,enemies[i].y)
  if ((frame%enemies[i].animspd)-enemies[i].animoff)==0 then
   enemies[i].anim=enemies[i].anim+1
   if (enemies[i].anim>=4) then
	   enemies[i].anim=0
   end
   if (enemies[i].anim==3) then
    local r = flr((rnd(9)/4)%2)
    enemies[i].anim=enemies[i].anim + r
   end
  end
 end 
